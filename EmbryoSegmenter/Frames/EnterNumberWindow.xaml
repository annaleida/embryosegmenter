<Window x:Class="EmbryoSegmenter.Frames.EnterNumberWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Enter number" Height="140" Width="280" Loaded="Window_Loaded" IsVisibleChanged="Window_IsVisibleChanged" >
    <Grid Margin="0">
        <TextBox Height="25" Margin="12,12,12,0" Name="txtNew" VerticalAlignment="Top" TextChanged="txtNew_TextChanged"/>
        <Button Name="btnOk" Click="btnOk_Click" Margin="64,55,0,0" Height="25" VerticalAlignment="Top" HorizontalAlignment="Left" Width="60" IsDefault="True">Ok</Button>
        <Button Margin="130,55,0,0" Name="btnCancel" HorizontalAlignment="Left" Width="60" Click="btnCancel_Click" Height="25" VerticalAlignment="Top">Cancel</Button>
    </Grid>
</Window>
